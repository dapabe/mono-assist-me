{
  "$schema": "https://turbo.build/schema.v1.json",
  "experimentalUI": true,
  "pipeline": {
    "clean": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["**/*.{ts,tsx,js,jsx}"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "node_modules/.cache/metro/**"]
    },
    "dev": {
      "passThroughEnv": ["*"],
      "interactive": true,
      "cache": false,
      "persistent": true
    }
  }
}
